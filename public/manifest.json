{
  "manifest_version": 2,
  "name": "Tabify",
  "description": "Beautiful new tab with nice features",
  "version": "0.0.1",
  "icons": {
    "16": "assets/img/logo.png",
    "48": "assets/img/logo.png",
    "128": "assets/img/logo.png"
  },
  "permissions": [
    "storage"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-inline'",
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}